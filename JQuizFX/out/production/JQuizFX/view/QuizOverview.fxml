<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.QuizOverview">
   <children>
      <AnchorPane prefHeight="198.0" prefWidth="339.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="quizTitleLabel" layoutX="146.0" layoutY="23.0" text="Quiz title" AnchorPane.topAnchor="23.0" />
            <ButtonBar layoutX="45.0" layoutY="55.0" prefHeight="40.0" prefWidth="233.0" AnchorPane.topAnchor="55.0">
              <buttons>
                <Button fx:id="play" mnemonicParsing="false" onAction="#playButton" text="Play!" />
                  <Button fx:id="rank" mnemonicParsing="false" onAction="#rankButton" text="Rank" />
                  <Button fx:id="exit" mnemonicParsing="false" onAction="#exitButton" text="Exit" />
              </buttons>
            </ButtonBar>
            <Label fx:id="quoteLabel" layoutX="87.0" layoutY="108.0" text="Will you stand the test of time?" AnchorPane.topAnchor="108.0" />
            <Label fx:id="quoteLabel1" layoutX="14.0" layoutY="156.0" text="Player selection" AnchorPane.topAnchor="156.0">
               <graphic>
                  <MenuButton fx:id="playerChooser" mnemonicParsing="false" text="Player">
                    <items>
                      <MenuItem fx:id="newPlayer" mnemonicParsing="false" onAction="#newPlayer" text="New Player" />
                    </items>
                  </MenuButton>
               </graphic>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
